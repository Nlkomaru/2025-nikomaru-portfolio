{
    "name": "2025-nikomaru-portfolio",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prepare": "panda codegen",
        "dev": "next dev --turbopack",
        "build": "next build",
        "build:cf": "opennextjs-cloudflare build",
        "build:storybook": "storybook build",
        "build:all": "pnpm dlx concurrently \"pnpm build:cf\" \"pnpm build:storybook\"",
        "start": "next start",
        "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
        "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
        "dev:storybook": "storybook dev -p 6006",
        "check": "biome check ./ --write"
    },
    "dependencies": {
        "@ark-ui/react": "^5.15.1",
        "@fontsource/zen-kaku-gothic-antique": "^5.2.6",
        "@opennextjs/cloudflare": "^1.3.0",
        "hono": "^4.8.2",
        "motion": "^12.19.1",
        "next": "15.3.3",
        "next-themes": "^0.4.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.0.0",
        "@chromatic-com/storybook": "^4.0.1",
        "@pandacss/dev": "^0.54.0",
        "@storybook/addon-a11y": "^9.0.12",
        "@storybook/addon-docs": "^9.0.12",
        "@storybook/addon-onboarding": "^9.0.12",
        "@storybook/addon-vitest": "^9.0.12",
        "@storybook/react": "^9.0.12",
        "@storybook/react-vite": "^9.0.12",
        "@types/node": "^20.19.1",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitest/browser": "^3.2.4",
        "@vitest/coverage-v8": "^3.2.4",
        "playwright": "^1.53.1",
        "storybook": "^9.0.12",
        "typescript": "^5",
        "vitest": "^3.2.4",
        "wrangler": "^4.20.3"
    },
    "packageManager": "pnpm@10.12.1"
